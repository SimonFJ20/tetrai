{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "removeComments": true,
        "outDir": "./dist/",
        "watch": true,
        "jsx": "react",
        "sourceMap": true
    },
    "include": ["src/**/*"]
}